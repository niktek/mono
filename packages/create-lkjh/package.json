{
	"name": "create-lkjh",
	"version": "0.1.9",
	"description": "Creates Skeleton UI apps on top of Svelte Kit, Vite (Svelte) and Astro",
	"keywords": [],
	"author": "niktek <nik@niktek.io>",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"bin": "./dist/bin.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*",
		"package.json",
		"templates/*"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "vitest dev",
		"lint": "eslint --ext .ts,.js,.mjs,.cjs .",
		"prepack": "unbuild",
		"release": "standard-version && git push --follow-tags && npm publish",
		"test": "pnpm lint && vitest run --coverage"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/prompts": "^2.4.1",
		"@vitest/coverage-c8": "^0.24.3",
		"eslint": "^8.25.0",
		"standard-version": "^9.5.0",
		"typescript": "^4.8.4",
		"unbuild": "^0.9.2",
		"vitest": "^0.24.3"
	},
	"dependencies": {
		"create-svelte": "2.0.0-next.190",
		"fs-extra": "^10.1.0",
		"kleur": "^4.1.5",
		"mri": "^1.2.0",
		"prompts": "^2.4.2",
		"which-pm-runs": "^1.1.0"
	}
}
