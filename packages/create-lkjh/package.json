{
	"name": "create-lkjh",
	"version": "0.1.14-beta.3",
	"description": "Creates Skeleton UI apps on top of Svelte Kit, Vite (Svelte) and Astro",
	"keywords": [],
	"author": "niktek <nik@niktek.io>",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"bin": {
		"skel": "dist/bin.mjs"
	},
	"files": [
		"dist",
		"bin",
		"package.json",
		"templates/*"
	],
	"scripts": {
		"prepublishOnly": "npm run build",
		"dev": "esno src/bin.ts",
		"build": "unbuild",
		"stub": "unbuild --stub",
		"release": "bumpp && npm publish",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/prompts": "^2.4.1",
		"@types/which-pm-runs": "^1.0.0",
		"@vitest/coverage-c8": "^0.24.3",
		"bumpp": "^8.2.1",
		"create-svelte": "2.0.0-next.190",
		"eslint": "^8.25.0",
		"esno": "^0.16.3",
		"fs-extra": "^10.1.0",
		"kleur": "^4.1.5",
		"mri": "^1.2.0",
		"prompts": "^2.4.2",
		"standard-version": "^9.5.0",
		"typescript": "^4.8.4",
		"unbuild": "^0.9.4",
		"which-pm-runs": "^1.1.0"
	}
}
