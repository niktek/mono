{
	"name": "create-skeleton-app",
	"version": "0.0.1",
	"description": "Use this CLI app to setup a new Skeleton application in a new SvelteKit project.",
	"repository": {
		"type": "git",
		"url": "https://github.com/skeletonlabs/skeleton",
		"directory": "packages/create-skeleton-app/"
	},
	"homepage": "https://www.skeleton.dev",
	"keywords": [
		"skeleton",
		"svelte",
		"sveltekit",
		"svelte-kit"
	],
	"author": "Skeleton Admin <admin@skeletonlabs.co>",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"bin": {
		"skel": "dist/bin.mjs"
	},
	"files": [
		"dist",
		"bin",
		"package.json",
		"templates/*"
	],
	"scripts": {
		"prepublishOnly": "npm run build",
		"dev": "tsx src/bin.ts",
		"build": "unbuild && tsx scripts/copyassets.ts",
		"stub": "unbuild --stub",
		"release": "bumpp && npm publish",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/prompts": "^2.4.1",
		"@types/which-pm-runs": "^1.0.0",
		"@vitest/coverage-c8": "^0.24.5",
		"bumpp": "^8.2.1",
		"eslint": "^8.27.0",
		"fs-extra": "^10.1.0",
		"kleur": "^4.1.5",
		"mri": "^1.2.0",
		"ora": "^6.1.2",
		"prompts": "^2.4.2",
		"standard-version": "^9.5.0",
		"tsx": "^3.11.0",
		"typescript": "^4.9.3",
		"unbuild": "^0.9.4",
		"which-pm-runs": "^1.1.0"
	},
	"dependencies": {
		"create-svelte": "2.0.0-next.191"
	}
}
